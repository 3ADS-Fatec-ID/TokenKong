            <Insets />
         </VBox.margin>
         <children>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/icons/dark/home-outline.png" />
               </image>
            </ImageView>
            <Label text="Home / ">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/icons/dark/view-grid-outline.png" />
               </image>
            </ImageView>
            <Label text="Categories" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="30.0" right="30.0" />
   </padding>
</VBox>
