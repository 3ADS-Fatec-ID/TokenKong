            <ScrollPane hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
               <content>
                  <VBox alignment="TOP_RIGHT" prefWidth="160.0">
                     <children>
                        <Button fx:id="toggle_drawer2" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#toggleDrawer" prefHeight="24.0" prefWidth="160.0" styleClass="drawer_button" textFill="WHITE">
