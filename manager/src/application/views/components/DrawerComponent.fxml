                              </ImageView>
                           </graphic>
                           <font>
                              <Font name="Poppins Medium" size="16.0" />
                           </font>
                        </Button>
                        <Button fx:id="home_button1" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#showHomePage" prefHeight="24.0" prefWidth="160.0" styleClass="drawer_button" text="Home" textFill="WHITE" VBox.vgrow="ALWAYS">
                           <graphic>
                              <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../assets/icons/light/home-outline.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <font>
                              <Font name="Poppins Medium" size="16.0" />
                           </font>
                        </Button>
                        <Button fx:id="categories_button1" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#showCategoriesPage" prefHeight="24.0" prefWidth="160.0" styleClass="drawer_button" text="Categories" textFill="WHITE">
