                              </ImageView>
                           </graphic>
                           <font>
                              <Font name="Poppins Medium" size="16.0" />
                           </font>
                        </Button>
                        <Button fx:id="products_button1" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#showProductsPage" prefHeight="24.0" prefWidth="160.0" styleClass="drawer_button" text="Products" textFill="WHITE">
